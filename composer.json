{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "version": "2.1.5",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "leafo/scssphp": "dev-master",
        "ornicar/gravatar-bundle": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "doctrine/migrations": "dev-master@dev",
        "doctrine/doctrine-migrations-bundle": "dev-master@dev",
        "phpoption/phpoption": "1.4.x",
        "knplabs/knp-markdown-bundle": "1.2.*@dev",
        "beberlei/lite-cqrs": "1.1.*@dev",
        "bcrm/www-content": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "endroid/qrcode-bundle": "1.5.1",
        "kriswallsmith/buzz": "dev-master@dev",
        "coderbyheart/mailchimp-bundle": "dev-master@dev",
        "nesbot/carbon": "1.17.0",
        "dothiv/dothiv-moneyformat-bundle": "1.x"
    },
    "require-dev": {
      "phpunit/phpunit": "4.x"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:BCRM/www-content.git"
        }
    ]
}
